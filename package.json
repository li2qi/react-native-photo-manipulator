{
  "name": "react-native-photo-manipulator",
  "version": "1.9.2",
  "description": "React Native Image Processing API to edit photo programmatically for Android and iOS.",
  "source": "./src/index.ts",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "ci": "CI=true && yarn typecheck && yarn lint && yarn test --maxWorkers=2 --coverage",
    "example": "yarn workspace react-native-photo-manipulator-example",
    "test": "jest",
    "typecheck": "tsc",
    "prepare": "bob build",
    "release": "release-it"
  },
  "keywords": [
    "react-native",
    "react native",
    "image editor",
    "image processing",
    "image manipulator",
    "photo editor",
    "photo processing",
    "photo manipulator"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guhungry/react-native-photo-manipulator.git"
  },
  "author": "Woraphot Chokratanasombat <guhungry@gmail.com> (https://github.com/guhungry)",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.0.0",
    "@evilmartians/lefthook": "^1.7.0",
    "@react-native/eslint-config": "^0.75.0",
    "@release-it/conventional-changelog": "^8.0.0",
    "@types/color-rgba": "^2.1.3",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.3.0",
    "commitlint": "^19.0.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-extended": "^4.0",
    "prettier": "^3.0.3",
    "react": "^18.3.0",
    "react-native": "^0.75.0",
    "react-native-builder-bob": "^0.30.0",
    "release-it": "^17.0.0",
    "turbo": "^2.0.11",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "color-rgba": "^3.0.0"
  },
  "resolutions": {
    "@types/react": "^18.3.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example"
  ],

  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  }
}
